#include<stdio.h>
main()
{
  int m;
  printf("enter the marks of the student\n");
  scanf("%d",&m);
  if(m>=0 && m<=39)
  printf("fail\n");
  else if(m>=40 && m<=59)
  printf("pass\n");
  else if(m>=60 && m<=79)
  printf("1st class\n");
  else if(m>=80 && m<=100)
  printf("distinction\n");
  else
  printf("invalid marks\n");


}





