#include<stdio.h>
main()
{
  int n1,n2,sum,sub,mul,div;
  char ch;
  printf("enter 2 num\n");
  scanf("%d%d",&n1,&n2);
  printf("enter the choice\n");
  scanf("%c%c",&ch,&ch);
  switch(ch)
  {
   case '+':sum=n1+n2;
          printf("sum=%d",sum);
          break;

  case '-':sub=n1-n2;
          printf("sub=%d",sub);
          break;
  case '*':mul=n1*n2;
          printf("mul=%d",mul);
          break;
  case '/':div=n1/n2;
          printf("div=%d",div);
          break;
    default:printf("invalid choice");
           break;
           }
           }
